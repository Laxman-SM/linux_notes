#interactive ssh login
	ssh -i ~/.ssh/aws-dev.pem ec2-user@10.0.101.230

#turn on/off sshd
	chkconfig sshd on-off
	service sshd status

#configure ssh key logon
	add user
	change user password
	create ~/.ssh directory and authorized_keys file
	add public ssh key
	change permissions to 700
	modify /etc/ssh/sshd_config
	uncomment rsa and pub auth lines